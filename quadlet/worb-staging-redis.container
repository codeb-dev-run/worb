# worb Redis - Quadlet Configuration
# Generated by CodeB CLI v7.0.8
# Environment: staging
# Network: codeb-network (DNS-based container communication)

[Unit]
Description=worb Redis staging Container
After=network-online.target

[Container]
Image=docker.io/library/redis:7-alpine
ContainerName=worb-staging-redis
PublishPort=6380:6379
Network=codeb-network

Volume=worb-staging-redis-data:/data:Z
HealthCmd=redis-cli ping
HealthInterval=30s
HealthTimeout=10s
HealthRetries=3
HealthStartPeriod=60s

[Service]
Restart=always
RestartSec=10s
TimeoutStartSec=300

[Install]
WantedBy=multi-user.target default.target